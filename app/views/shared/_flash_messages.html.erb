<div class='container'>
  <% flash.each do |key, value| %>
    <div class='alert alert-dismissable alert-<%= key.to_s %> col-md-5'>
      <button type="button"
        class="close"
        data-dismiss="alert"
        aria-hidden="true">
        &times;
      </button>
      <%= value %>
    </div>
  <% end %>
</div>
